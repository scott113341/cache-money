{
  "name": "cache-money",
  "version": "0.1.1",
  "description": "File-system cache with TTL.",
  "author": "Scott Hardy",
  "license": "MIT",
  "repository": "git@github.com:scott113341/cache-money.git",
  "homepage": "https://github.com/scott113341/cache-money",
  "bugs": "https://github.com/scott113341/cache-money/issues",
  "keywords": [],
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "babel --copy-files --out-dir lib/ src/",
    "prebuild": "npm run clean",
    "clean": "shx rm -rf lib/ && shx mkdir lib/",
    "lint": "semistandard --fix",
    "test": "npm run build && node lib/test/index.js",
    "test:ci": "node lib/test/index.js && semistandard"
  },
  "dependencies": {
    "bluebird": "^3.4.6"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-async-to-generator": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.16.0",
    "np": "^2.9.0",
    "semistandard": "^9.0.0",
    "shx": "^0.1.4",
    "tape": "^4.6.0"
  },
  "semistandard": {
    "ignore": [
      "/lib/"
    ]
  }
}
